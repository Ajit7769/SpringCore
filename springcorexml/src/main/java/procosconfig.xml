		<property name="city" value="latur" />
		<property name="mobile_no" value="7769554256" />
		<property name="product">
			<list>
				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product1">

					<property name="id" value="1" />
					<property name="name" value="t-shirt" />
					<property name="price" value="350.00" />

				</bean>

				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product2">

					<property name="id" value="2" />
					<property name="name" value="Gold Chain" />
